{
	"env": {
		"browser": false,
		"commonjs": true,
		"es6": true,
		"node": true,
		"mocha": true
	},
	"parserOptions": {
		"ecmaVersion": 2018,
		"ecmaFeatures": {
			"jsx": true
		},
		"sourceType": "module"
	},
	"rules": {
		"no-const-assign": "warn",
		"no-this-before-super": "warn",
		"no-undef": "warn",
		"no-unreachable": "warn",
		"no-var": "error",
		"quotes": [
			"warn",
			"backtick"
		],
		"constructor-super": "warn",
		"valid-typeof": "warn",
		"indent": [
			"error",
			2,
			{
				"SwitchCase": 0
			}
		]
	}
}